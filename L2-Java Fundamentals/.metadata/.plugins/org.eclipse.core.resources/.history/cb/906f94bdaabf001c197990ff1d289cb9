package patterns;

import java.util.Scanner;

public class Pattern16 {

	public static void main(String[] args) {

		//		Pattern to Print
		//		A
		//		BC
		//		CDE

		int num, i, j;

		Scanner scanner = new Scanner(System.in);
		num = scanner.nextInt();

		i = 1;
		while (i <= num) {
			j = 1;
			while (j <= i) {
				char ch = (char)('A' + num -1);
				System.out.print('A');
				num--;
				j++;
			}
			System.out.print("\n");
			i++;
		}

	}

}
