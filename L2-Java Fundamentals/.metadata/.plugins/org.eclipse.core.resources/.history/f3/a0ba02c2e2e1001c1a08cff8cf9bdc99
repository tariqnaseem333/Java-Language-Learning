package fundamentals;

public class RelationalLogicalOperators {

	public static void main(String[] args) {
		int a = 10, b = 10;
		
//		Relational Operators f,f,t,f,t,t
		System.out.println(a > b);
		System.out.println(a < b);
		System.out.println(a == b);
		System.out.println(a != b);
		System.out.println(a >= b);
		System.out.println(a <= b);
		
//		Logical Operators
//		System.out.println(a>=10 && b>10);
//		System.out.println(a>=10 || b>10);
//		System.out.println(!(a>b));

	}

}
