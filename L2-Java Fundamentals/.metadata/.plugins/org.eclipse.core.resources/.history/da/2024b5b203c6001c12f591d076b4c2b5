package patterns;

import java.util.Scanner;
import java.lang.Math;

public class Pattern34 {

	public static void main(String[] args) {

//		For Printing Pattern
//		1357
//		3571
//		5713
//		7135
			


		int num;
	
		Scanner scanner = new Scanner(System.in);
		num = scanner.nextInt();
		
		int i = 1;
		while ( i <= num ) {
			
//			For Printing Increasing Number
			int j = 1;
			int p = 1;
			while ( j <= num - i + 1 ) {
				
				System.out.print(p);
				p = p + 2;
				j++;
			}
			
			System.out.print("\n");
			i++;
		}
		


	}

}
