package fundamentals;

public class RelationalLogicalOperators {

	public static void main(String[] args) {
//		int a = 10, b = 10;
		
//		Relational Operators
//		System.out.println(a > b);
//		System.out.println(a < b);
//		System.out.println(a == b);
//		System.out.println(a != b);
//		System.out.println(a >= b);
//		System.out.println(a <= b);
		
//		Logical Operators
//		System.out.println(a>=10 && b>10);
//		System.out.println(a>=10 || b>10);
//		System.out.println(!(a>b));
		
        double a = 55.5;
        int b = 55;
        a = a % 10;
        b = b % 10;
        System.out.println(a + " "  + b);

	}

}
