package patterns;

import java.util.Scanner;

public class Pattern14 {

	public static void main(String[] args) {
		
//		Pattern to Print
//		ABC
//		BCD
//		CDE
		
		int num, i, j;
        
        Scanner scanner = new Scanner(System.in);
        num = scanner.nextInt();
        
        i = 1;
        while (i <= num) {
            j = 1;
            while (j <= i) {
            	char jthChar = (char)('A' + i + j - 2);
                System.out.print(jthChar);
                j++;
            }
            System.out.print("\n");
            i++;
        }

	}

}
