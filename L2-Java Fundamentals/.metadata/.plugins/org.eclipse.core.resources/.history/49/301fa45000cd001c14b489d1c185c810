package functions;

import java.util.Scanner;

public class FahrenheitToCelsiusTable {

	public static void main(String[] args) {
		
		Scanner s = new Scanner(System.in);
		int start = s.nextInt();
		int end = s.nextInt();
		int step = s.nextInt();
		
		int c = 0; 
		for ( int f = start; f <= end; f+=step ) {
            c = (5/9)*(f - 32) ;
            System.out.println( f + "\t" + c );

        }

	}

}
