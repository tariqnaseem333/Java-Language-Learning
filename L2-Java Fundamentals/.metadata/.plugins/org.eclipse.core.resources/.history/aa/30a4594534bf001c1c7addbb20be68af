package patterns;

import java.util.Scanner;

public class Pattern4 {

	public static void main(String[] args) {
		
//		Print Square Pattern
//		321
//		321
//		321
		
		int n, i, j;
		
		Scanner scanner = new Scanner(System.in);
		n = scanner.nextInt();
		
		i = 1;
		while( i <= n ) {
			j = n;
			while( j >= 1 ) {
				System.out.print(j);
				j--;
			}
			System.out.print("\n");
			i++;
		}

	}

}
