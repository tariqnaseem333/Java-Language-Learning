package patterns;

import java.util.Scanner;

public class Pattern11 {

	public static void main(String[] args) {
		
//		Pattern to Print
//		1
//		22
//		333
		
		int num, i, j;
        
        Scanner scanner = new Scanner(System.in);
        num = scanner.nextInt();
        
        i = 1;
        while (i <= num) {
            j = i;
            while (j >= 1) {
                System.out.print(j);
                j--;
            }
            System.out.print("\n");
            i++;
        }

	}

}
