package fundamentalsAdvanced;

import java.util.Scanner;

public class AllPrimeNumbers {

	public static void main(String[] args) {
		
		int num;
		
		Scanner scanner  = new Scanner(System.in);
		num = scanner.nextInt();
		
		int i = 2;
		while( i <= ( num -1 ) ) {
			if ( num % i != 0 ) {
				System.out.println(i);
			}
			i++;
		}

	}

}
