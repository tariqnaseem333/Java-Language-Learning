package patterns;

import java.util.Scanner;

public class Pattern16 {

	public static void main(String[] args) {

		//		Pattern to Print
		//		A
		//		BC
		//		CDE

		int num, i, j;

		Scanner scanner = new Scanner(System.in);
		num = scanner.nextInt();

		i = num;
		while (i >= 1) {
			char ch = (char)('A' + i -1);
			j = 1;
			while (j <= i) {
				System.out.print((char)(ch -1));
				j++;
			}
			System.out.print("\n");
			i--;
		}

	}

}
