package fundamentals;

import java.util.Scanner;

public class HelloWorld {
	
	public static void main(String[] args) {
		
//		println prints a new line at the end
//		print prints on the same line
		
//		System.out.println("Hello World");
//		System.out.println("Hello World");
//		System.out.println("Hello World");
		
		int i=10;
	    while((i=i-1)>0)
	    {
	        System.out.print(i);
	        if(i%5==0)
	            return;
	    }
	}

}